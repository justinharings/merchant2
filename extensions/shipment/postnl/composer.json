{
    "name": "thirtybees/postnl-api-php",
    "description": "PostNL REST API PHP Bindings",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Michael Dekker",
            "email": "michael@thirtybees.com"
        }
    ],
    "require": {
        "php": "^5.6 || ^7.0",
        "ext-json": "*",
        "sabre/xml": "~1.5.0",
        "guzzlehttp/psr7": "^1.4",
        "setasign/fpdi": "^2.0",
        "setasign/fpdf": "^1.8",
        "psr/cache": "^1.0",
        "psr/log": "^1.0"
    },
    "require-dev": {
        "php": "^5.6 || ^7.0",
        "phpunit/phpunit": "^5.0",
        "wappr/logger": "^1.2",
        "guzzlehttp/guzzle": "^6.3",
        "cache/void-adapter": "^1.0"
    },
    "suggest": {
        "guzzlehttp/guzzle": "Process requests faster + auto retry failed requests"
    },
    "autoload": {
        "psr-4": {
            "ThirtyBees\\PostNL\\": "src/"
        }
    }
}
